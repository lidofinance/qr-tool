    "compact": "inliner build/index.html > build/index_compact.html",
    "clean": "rimraf ./build/*"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ]
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
