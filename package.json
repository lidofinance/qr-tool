{
  "name": "qr-tool",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@zxing/browser": "0.0.9",
    "@zxing/library": "0.18.6",
    "bulma": "0.9.3",
    "gifshot": "0.4.5",
    "mini-lz4": "3.0.0",
    "tiny-async-pool": "1.2.0",
    "typescript": "4.1.2"
  },
  "devDependencies": {
    "@types/node": "12.0.0",
    "@types/tiny-async-pool": "1.0.0",
    "inliner": "1.13.1",
    "parcel": "2.0.0",
    "posthtml-include": "1.7.2",
    "rimraf": "^3.0.2"
  },
  "license": "UNLICENSED",
  "scripts": {
    "start": "parcel --no-cache --port 3000 ./src/index.html ",
